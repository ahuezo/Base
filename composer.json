{
    "name": "ahuezo/typi-base",
    "description": "Multilingual CMS built with Laravel 5",
    "keywords": ["cms", "multilingual", "laravel", "typi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel De Backer",
            "email": "samuel@typidesign.be",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.3.*",
        "barryvdh/laravel-debugbar": "~2.0",
        "edvinaskrucas/notification": "~5.1.0",
        "guzzlehttp/guzzle": "~6.0",
        "laracasts/presenter": "~0.2.1",
        "laracasts/utilities": "~2.1.0",
        "bkwld/croppa": "~4.4.0",
        "maatwebsite/laravel-sidebar": "~1.1.1",
        "roumen/feed": "~2.10.1",
        "rinvex/repository": "2.0.*",
        "spatie/laravel-permission": "~1.5.0",
        "typicms/laravel-translatable-bootforms": "dev-master",
        "ahuezo/typi-nestablecollection": "~1.1.1",
        "ahuezo/typi-core": "~3.0.0",
        "ahuezo/typi-objects": "~3.0.0",
        "ahuezo/typi-blocks": "~3.0.0",
        "ahuezo/typi-dashboard": "~3.0.0",
        "ahuezo/typi-files": "~3.0.0",
        "ahuezo/typi-galleries": "~3.0.0",
        "ahuezo/typi-roles": "~3.0.0",
        "ahuezo/typi-history": "~3.0.0",
        "ahuezo/typi-menus": "~3.0.0",
        "ahuezo/typi-pages": "~3.0.0",
        "ahuezo/typi-settings": "~3.0.0",
        "ahuezo/typi-sitemap": "~3.0.0",
        "ahuezo/typi-translations": "~3.0.0",
        "ahuezo/typi-users": "~3.0.0"
    },
    "require-dev": {
        "laracasts/generators": "~1.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "TypiCMS\\Modules\\": "Modules/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php clear-compiled",
            "php artisan clear-html",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php clear-compiled",
            "php artisan clear-html",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dev"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
